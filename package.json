{
  "name": "whatpm-server",
  "version": "1.0.0",
  "description": "Express backend for what.pm",
  "author": "Nienke Dekker <nienkedekker@gmail.com>",
  "private": true,
  "scripts": {
    "start:dev": "NODE_ENV=development nodemon --exec babel-node src/bin/www",
    "start:prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "server:prod": "node ./dist/bin/www",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist"
  },
  "dependencies": {
    "async": "^2.6.1",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "debug": "^4.1.1",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.9",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "eslint": "^5.12.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  }
}
